        var ID;
        var getCourseDetail = async (id) => {
            ID = id;
            console.log("getcourse details called");
            try {
                let apiEndpoint =
                    `https://anheuserbuschinbev.sharepoint.com/sites/Sales-EUR/_api/web/lists/getbytitle('Video_Overview')/items?$filter= (ID eq ${id})`;
                //?$filter=Category eq 'home_section3'
                let response = await fetch(apiEndpoint, {
                    method: "GET",
                    headers: { Accept: "application/json;odata=verbose" },
                });

                if (response.ok) {
                    let data = await response.json();
                    //var items =data.d.results;
                    // console.log("data of dynamicCourse ", data);
                    if (data.d.results && data.d.results.length > 0) {
                        console.log("data is: ", data.d.results);
                        for (let i = 0; i < data.d.results.length; i++) {
                            const course = data.d.results[i];

                            $("#courseTitle").val(course.Title);
                            $("#courseDescription").val(course.description);
                            $("#uploadImg").attr("src", course.image);
                            $("#courseDuration").val(course.duration);
                        }
                    }

                } else {
                    throw new Error("Error fetching Dynamic_Contents data");
                }
            } catch (err) {
                console.error(err);
            }
        }

        var save = document.getElementById('savebtn');


        save.addEventListener('click', function () {
            var courseTitle = document.getElementById('courseTitle').value;
            var courseDescription = document.getElementById('courseDescription').value;
            var courseDuration = document.getElementById('courseDuration').value;

            var element = document.getElementById("uploadInput");
            var selectedFile = element.files[0];
            if (selectedFile) {
                const fileName1 = selectedFile.name;
                const sessionImage = _spPageContextInfo.webAbsoluteUrl + "/Shared Documents/" + fileName1;

                readsessionFile(ID, courseTitle, courseDescription, courseDuration,sessionImage)
            }
            UpdateListItem(ID, courseTitle, courseDescription, courseDuration,null)
        });




        function UpdateListItem(id, courseTitle, courseDescription, courseDuration,sessionImage) {
            // Display values (for debugging purposes)
            console.log("ID s", id);
            console.log('course Name:', courseTitle);
            console.log("description", courseDescription);
            console.log("duration", courseDuration);

            var clientContext = new SP.ClientContext(_spPageContextInfo.webAbsoluteUrl);
            var oList = clientContext.get_web().get_lists().getByTitle('course_home');
            var oListItem = oList.getItemById(id);

            // Set properties of the list item
            oListItem.set_item("Title", courseTitle);
            oListItem.set_item("duration", courseDuration);
            oListItem.set_item("description", courseDescription);
            if(sessionImage){
                oListItem.set_item("image", sessionImage);
            }



            oListItem.update();

            clientContext.executeQueryAsync(
                function () {
                    console.log("Item updated successfully.");


                    Swal.fire({
                        title: "updated successfully!",
                        icon: "success"
                    }).then(() => {
                        $('#exampleModal').modal('hide')
                    });
                },
                function (sender, args) {
                    console.log("Error updating item: " + args.get_message());
                }
            );
        }

        var deleteCourse=async(id)=>{
            try {
                var clientContext = new SP.ClientContext(_spPageContextInfo.webAbsoluteUrl);
                var oList = clientContext.get_web().get_lists().getByTitle('Video_Overview');

                var oListItem = oList.getItemById(id);
                Swal.fire({
                    title: "Delete Course",
                    icon: "info",
                    showCancelButton: true,
                    }).then((result) => {
                        if (result.isConfirmed) {
                    // This code will be executed after the user interacts with the Swal dialog
                    oListItem.deleteObject();
                    
                    clientContext.executeQueryAsync(Function.createDelegate(this, onQuerySucceeded8), Function.createDelegate(this, onQueryFailed8));
                        }else if (result.isDismissed) {
                                // This code will be executed if the user clicks "Cancel" or outside the Swal dialog
                                console.log("Deletion canceled");
                                // You can add additional actions or remove this block if not needed
                            }
                    })
                        function onQuerySucceeded8() {
                        console.log('Item deleted ');
                        fetchDynamicContent()
                        }
                        
                        function onQueryFailed8(sender, args) {
                            console.log('Request failed. ' + args.get_message() + '\n' + args.get_stackTrace());
                        }
            } catch (error) {
                console.log('error for deletion is',error)
            }
        }

    </script>

    <script>

        function readsessionFile(ID, courseTitle, courseDescription, courseDuration,sessionImage) {

            var element = document.getElementById("uploadInput");
            var file = element.files[0];

            var parts = element.value.split("\\");
            var fileName = parts[parts.length - 1];
            console.log("filename is", fileName);
            //Read File contents using file reader  
            var reader = new FileReader();
            reader.onload = function (e) {
                uploadFile(e.target.result, fileName,ID, courseTitle, courseDescription, courseDuration,sessionImage);
            }
            reader.onerror = function (e) {
                alert(e.target.error);
            }
            reader.readAsArrayBuffer(file);
        }

        function uploadFile(arrayBuffer, fileName,ID, courseTitle, courseDescription, courseDuration,sessionImage) {
            //Get Client Context,Web and List object.  
            var clientContext = new SP.ClientContext();
            var oWeb = clientContext.get_web();
            var oList = oWeb.get_lists().getByTitle('Documents');

            //Convert the file contents into base64 data  
            var bytes = new Uint8Array(arrayBuffer);
            var i, length, out = '';
            for (i = 0, length = bytes.length; i < length; i += 1) {
                out += String.fromCharCode(bytes[i]);
            }
            var base64 = btoa(out);
            //Create FileCreationInformation object using the read file data  
            var createInfo = new SP.FileCreationInformation();
            createInfo.set_content(base64);
            createInfo.set_url(fileName);


            //Add the file to the library  
            var uploadedDocument = oList.get_rootFolder().get_files().add(createInfo)
            // Get the list item associated with the file.
            var listItem = uploadedDocument.get_listItemAllFields();



            //Load client context and execcute the batch  
            clientContext.load(uploadedDocument);
            clientContext.executeQueryAsync(function () {
                console.log('File Uploaded Successfully.');

                UpdateListItem(ID, courseTitle, courseDescription, courseDuration,sessionImage)

                // var modal1 = document.getElementById("addexpertModal");
                // modal1.style.display = "none";
            }, QueryFailure);
            function QueryFailure(sender, args) {
                console.log('Request failed with error message - ' + args.get_message() + ' . Stack Trace - ' + args.get_stackTrace());
                UpdateListItem(ID, courseTitle, courseDescription, courseDuration,sessionImage)

                // expertImage=_spPageContextInfo.webAbsoluteUrl + "/Shared Documents/default-expert.png"
                // alert("Request failed with error message - " + args.get_message() + " . Stack Trace - " + args.get_stackTrace());
                //createSessionItem()
            }
        }

    </script>






</body>

</html>
