    <script src="https://anheuserbuschinbev.sharepoint.com/sites/Sales-EUR/Assets/js/owl.carousel.js"></script>
    <script>
        document
            .querySelector("[data-automation-id='contentScrollRegion']")
            .addEventListener("scroll", (event) => {
                if (
                    document.querySelector("[data-automation-id='contentScrollRegion']")
                        .scrollTop > 30
                ) {
                    $("nav").addClass("newclass");
                } else {
                    $("nav").removeClass("newclass");
                }
            });
    </script>


    <script>

        function showTab(tabId) {
            // Hide all tabs and tab contents
            const tabs = document.querySelectorAll('.tab');
            const tabContents = document.querySelectorAll('.tab-content');

            tabs.forEach(tab => tab.classList.remove('active'));
            tabContents.forEach(content => content.classList.remove('active'));

            // Show the selected tab and tab content
            const selectedTab = document.getElementById(tabId);
            const selectedTabContent = document.getElementById(tabId);

            if (selectedTab && selectedTabContent) {
                selectedTab.classList.add('active');
                selectedTabContent.classList.add('active');
            }
        }

    </script>
    <script>
        $(document).ready(function () {
            $('li.tab').click(function () {
                // Remove the 'selected' class from all siblings
                $(this).siblings().removeClass('active');

                // Add the 'selected' class to the clicked li
                $(this).addClass('active');
            });
        });
    </script>

    <script>
        function deleteItem() {
            const itemsContainer = document.getElementById('itemsContainer');
            const checkboxes = document.querySelectorAll('.form-check-input:checked');
            checkboxes.forEach(checkbox => {
                const itemDiv = checkbox.closest('.item');
                itemsContainer.removeChild(itemDiv);
            });
        }
    </script>




    <script>
        function handleImageChange() {
            const uploadInput = document.getElementById('uploadInput');
            const profileImage = document.getElementById('profileImage');

            const file = uploadInput.files[0];

            if (file) {
                const reader = new FileReader();
                reader.onload = function (e) {
                    profileImage.innerHTML = '<img src="' + e.target.result + '" alt="Profile Picture">';
                };
                reader.readAsDataURL(file);
            }
        }
    </script>
    <script>
        function handleImageChange1() {
            const uploadInput = document.getElementById('uploadInput1');
            const profileImage = document.getElementById('profileImage1');

            const file = uploadInput.files[0];

            if (file) {
                const reader = new FileReader();
                reader.onload = function (e) {
                    profileImage.innerHTML = '<img src="' + e.target.result + '" alt="Profile Picture">';
                };
                reader.readAsDataURL(file);
            }
        }
    </script>
    <!-------dynamic cards start here--------->
    <script>
         $(document).ready(function () {
            var urlParams = new URLSearchParams(window.location.search);
 
            // Get the values of the query parameters
            var param1 = urlParams.get('param1');
            if(!param1){
                console.log('default course edit')
            }
            if(param1==='testimonial'){
                // Get the button element by its ID
            var button = document.getElementById('nav-profile-tab');

            // Trigger a click event on the button
            button.click();
            }
        })

    </script>



    <script>

        console.log("dynamic script is loaded");
        document.addEventListener("DOMContentLoaded", fetchDynamicContent);

        var uniqueArray=[];
        async function fetchDynamicContent() {
            try {
                let apiEndpoint =
                    "https://anheuserbuschinbev.sharepoint.com/sites/Sales-EUR/_api/web/lists/getbytitle('Video_Overview')/items?$orderby=ID asc";
                //?$filter=Category eq 'home_section3'
                let response = await fetch(apiEndpoint, {
                    method: "GET",
                    headers: { Accept: "application/json;odata=verbose" },
                });

                if (response.ok) {
                    let data = await response.json();
                    //var items =data.d.results;
                    // console.log("data of dynamicCourse ", data);
                    if (data.d.results) {
                        renderDynamicContent(data.d.results);
                    }
                    else {

                    }
                    console.log("dynamic data is", data.d.results);
                } else {
                    throw new Error("Error fetching Dynamic_Contents data");
                }
            } catch (err) {
                console.error(err);
            }
        }

        function renderDynamicContent(contents) {
            //console.log("Contents length is:", contents);

            var user_data = "";
            for (let i = 0; i < contents.length; i++) {
                if(!uniqueArray.includes(contents[i].module_id)){
                    user_data += ` <div class="col-md-3 mb-4">
                            <div class="card">
                              <div class="card-img">
                                <img src="${contents[i].content_img}" class="img-fluid">
                                
                                <div id="rollingIcon266" class="rolling"></div>
                              </span>
                              </div>
                              <div class="card-img-overlay">
                                <h4>${contents[i].Title}</h4>
                                <p>${contents[i].description_content}</p>
                                <p><span>by ${contents[i].tutor || "tutor"}</span></p>

                                <div class="btn_cls" data-bs-toggle="modal" data-bs-target="#exampleModal">
                                  <button onclick='getCourseDetail(${contents[i].ID})' disabled>
                                    Edit/ Modify
                                  </button>
                                 
                                </div>
                                <span class="delete-icon" onclick='deleteCourse(${contents[i].ID});'>
