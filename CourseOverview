    <script>
        document
            .querySelector("[data-automation-id='contentScrollRegion']")
            .addEventListener("scroll", (event) => {
                if (
                    document.querySelector("[data-automation-id='contentScrollRegion']")
                        .scrollTop > 30
                ) {
                    $("nav").addClass("newclass");
                } else {
                    $("nav").removeClass("newclass");
                }
            });
    </script>


    <script>

        function showTab(tabId, mianId, contenType) {
            console.log(mianId);
            console.log('contenType', contenType);
            if (contenType == 'Quiz') {
                fetchDynmQuizzesContent();
            }
            $("#" + tabId).addClass('active').siblings().removeClass('active');
            $("#" + mianId).addClass('active').siblings().removeClass('active');
        }

    </script>

    <script>
        $(document).ready(function () {
            $('li.tab').click(function () {
                // Remove the 'selected' class from all siblings
                $(this).siblings().removeClass('active');

                // Add the 'selected' class to the clicked li
                $(this).addClass('active');
            });
        });
    </script>

    <script>
        // Recommended page

        $(document).ready(function () {
            fetchDynamicContentRecommPage();
        });

        var siteUrl = _spPageContextInfo.webAbsoluteUrl;
        async function fetchDynamicContentRecommPage() {
            try {
                let categoryFilter = "Recommended Courses";
                let listName = "course_home";
                let apiEndpoint = `${siteUrl}/_api/web/lists/getbytitle('${listName}')/items?$filter=category eq '${categoryFilter}'`;
                let response = await fetch(apiEndpoint, {
                    method: "GET",
                    headers: { Accept: "application/json;odata=verbose" },
                });

                if (response.ok) {
                    let data = await response.json();
                    // console.log("data top learners new", data);
                    recommContents(data.d.results)
                } else {
                    throw new Error("Error fetching Dynamic_Contents data");
                }
            } catch (err) {
                console.error(err);
            }
        }


        async function recommContents(contents) {
            let recomm_section = "";
            contents.forEach((item) => {
                recomm_section += `
      <li>
        <a href="#!">
            <div class="row">
                <div class="col-md-4">
                    <img
                        src="${item.vertical_thumbnail}">
                </div>
                <div class="col-md-8">
                    <div class="row">
                        <div class="col-md-10">
                            <div class="cource_des">
                                <h4>${item.Title}</h4>
                                <p>${item.tutor}</p>
                            </div>
                            <p><span>${item.duration}</span></p>
                        </div>
                        <div class="col-md-2">
                            <span>
                                <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30"
                                    viewBox="0 0 30 30" fill="none">
                                    <path fill-rule="evenodd" clip-rule="evenodd"
                                        d="M12.4621 9.33708C12.8282 8.97097 13.4217 8.97097 13.7879 9.33708L18.7879 14.3371C19.154 14.7032 19.154 15.2967 18.7879 15.6629L13.7879 20.6629C13.4217 21.029 12.8282 21.029 12.4621 20.6629C12.096 20.2967 12.096 19.7032 12.4621 19.3371L16.7991 15L12.4621 10.6629C12.096 10.2968 12.096 9.7032 12.4621 9.33708Z"
                                        fill="black" />
                                </svg>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
        </a>
    </li>`
            })
            $("#recomm_id").html(recomm_section)
        }
    </script>

