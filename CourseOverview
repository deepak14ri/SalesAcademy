    <script>

        $(document).ready(function () {
            fetchDynCoursePage();
            fetchDynmQuizzesContent();
        });

        var encodedCourseName;

        var siteUrl = _spPageContextInfo.webAbsoluteUrl;
        async function fetchDynCoursePage() {
            try {
                let listName = "Video_Overview";
                let apiEndpoint = `${siteUrl}/_api/web/lists/getbytitle('${listName}')/items?$filter=module_id eq '${moduleId}'`;
                let response = await fetch(apiEndpoint, {
                    method: "GET",
                    headers: { Accept: "application/json;odata=verbose" },
                });

                if (response.ok) {
                    let data = await response.json();
                    dynTabContents(data.d.results);

                } else {
                    throw new Error("Error fetching Dynamic_Contents data");
                }
            } catch (err) {
                console.error(err);
            }
        }

        async function fetchDynmQuizzesContent(indx, modulename) {
            try {
                let apiEndpoint =
                    `https://anheuserbuschinbev.sharepoint.com/sites/Sales-EUR/_api/web/lists/getbytitle('quizzes_list')/items?$filter=( course_id eq '${moduleId}' and module eq '${modulename}')`;
                let response = await fetch(apiEndpoint, {
                    method: "GET",
                    headers: { Accept: "application/json;odata=verbose" },
                });
                // console.log('api endpoint is')
                if (response.ok) {
                    let data = await response.json();
                    let data2 = rendDynmQuizzesContent(data.d.results, indx, modulename);
                    return data2;
                } else {
                    throw new Error("Error fetching fetchDynmQuizzesContent data");
                }
            } catch (err) {
                console.error(err);
            }
        }

        var buttonStyle = `
                background: #FFC700;
                font-weight: 600;
                border-radius: 7px;
                float: right;
                display: flex;
                width: 130px;
                height: 41px;
                padding: 10px 15px;
                justify-content: center;
                align-items: center;
                gap: 10px;
                flex-shrink: 0;
            `;

        var selectedOption; // Declare selectedOption in the outer scope
        var tabContentsDynamic = '';
        var quizContent = '';

        function rendDynmQuizzesContent(items, indexx, moduleName) {
            try {
                if (!items || items.length == 0) {
                    console.error('No quiz content available.');
                    return;
                }

                var questLength = items.length;
                quizContent = "";

                for (let i = 0; i < items.length; i++) {
                    let course_name = items[i].courseName0;
                    let module = items[i].module;
                    console.log('moduleName ', module);

                    let options = items[i].Choices.split(',').map(option => option.trim());
                    console.log('options: ', options);

                    let correctAnswer = items[i].Ans.split(': ')[1].trim();
                    correctAnswer = correctAnswer.trim()
                    console.log('correctAnswer: ', correctAnswer);
                    const optionLabels = ['A', 'B', 'C', 'D', 'E']; // Add more if needed
                    const correctAnswerIndex = optionLabels.indexOf(correctAnswer);

                    const optionsWithLabels = options.reduce((acc, option, index) => {
                        acc[optionLabels[index]] = option;
                        return acc;
                    }, {});

                    let correctAnswerOption = optionsWithLabels[correctAnswer];

                    let question = items[i].Question;
                    let currentQuestionIndex = 0;

                    function updateButtonVisibility() {
                        const prevButton = $(`#prev_id${currentQuestionIndex}`);
                        const nextButton = $(`#next_id${currentQuestionIndex}`);

                        prevButton.toggle(currentQuestionIndex > 0);

                        nextButton.toggle(currentQuestionIndex < items.length - 1);
                    }

                    quizContent += `
                    <div class="quize" id="quize_qution_${indexx + 1}_${i}" style="display:${i == 0 ? "block" : "none"}">
                        <h4>Interactive Quiz</h4>
                        <p>Question ${i + 1}/${questLength}</p>
                        <div class="quize_qution" >
                            <p>${question}</p>
                            <form id="myForm${i}_${indexx}" style="margin-left: 10px; onsubmit="return false;">
                                ${options.map((option, index) => `
                                    <label class=''>
                                        <input type="radio" name="options${i}_${indexx}" id="radio-${i}-${index}-${indexx}" value="${option}" > ${option}
                                    </label>
                                    <br>
                                `).join('')}
                                <br>
                                <input type="submit" value="Submit" style="${buttonStyle}" id="submitButton${i}_${indexx}" onclick="enableSubmitButton(${i}, '${correctAnswer}', ${questLength}, '${course_name}', '${moduleName}','${i}_${indexx}','${indexx}')">
                                <button id="next_id${i}_${indexx}" style="display: none;" class='buttonStyle' onclick="nextQuestion(event, ${i + 1}, ${questLength}, '_${indexx + 1}_${i + 1}')">Next Question</button>
                            </form>
                        </div>
                    </div>`;
                }
                return quizContent;
            } catch (err) {
                console.error(err);
            }
        }


        function nextQuestion(event, nextStep, questLength, nextQuesID) {
            event.preventDefault();
            $(`#myForm${nextQuesID} input[type=radio]`).prop('disabled', true);
            console.log('Current Step:', nextStep);
            if (questLength > nextStep) {
                $("#quize_qution" + nextQuesID).show().siblings().hide();
            } else {
                alert('Form submitted!');
            }
        }



        function selectOption(option) {
            selectedOption = option;
        }

        async function quizCheckAndUpdateItem(isComplete, quizScore, course_name, module_name) {
            let email_id = _spPageContextInfo.userLoginName;
            let apiBaseUrl = _spPageContextInfo.webAbsoluteUrl + `/_api/web/lists/getbytitle('quiz_completion')/items`;

            // Build the filter query to find items associated with the user, course, and module
            const filterQuery = `$filter=(Title eq '${email_id}' and courseName eq '${course_name}' and module eq '${module_name}')`;

            // Combine the base URL with the filter query
            const filteredApiUrl = `${apiBaseUrl}?${filterQuery}`;

            try {
                // Send a GET request to retrieve user-specific items
                let response = await fetch(filteredApiUrl, {
                    method: 'GET',
                    headers: {
                        'Accept': 'application/json;odata=verbose',
                    },
                });

                if (response.ok) {
                    // Check if an item exists for the user, course, and module
                    let data = await response.json();

                    if (data.d.results.length > 0) {
                        // If the item exists, update it
                        const existingItem = data.d.results[0];
                        const itemId = existingItem.Id;
                        await quizUpdateListItem(itemId, isComplete, quizScore, module_name);
                    } else {
                        // If the item doesn't exist, add a new item with a POST request
                        await quizCreateListItem(email_id, isComplete, course_name, quizScore, module_name);
                    }
                } else {
                    console.error('Error fetching user-specific items');
                }
            } catch (error) {
                console.error(error);
            }
        }

        function quizUpdateListItem(itemId, isComplete, Score, module_name) {
            var context = new SP.ClientContext(_spPageContextInfo.webAbsoluteUrl);
            var list = context.get_web().get_lists().getByTitle('quiz_completion');
            var item = list.getItemById(itemId);

            item.set_item('isQuizCompleted', isComplete);
            item.set_item('quizScore', Score);

            item.update();
            context.executeQueryAsync(
                function () {
                    console.log("Item updated successfully.");
                },
                function (sender, args) {
                    console.log("Error updating item: " + args.get_message());
                }
            );
        }

        function quizCreateListItem(email_id, isComplete, course_name, quizScore, module_name) {
            let context = new SP.ClientContext(_spPageContextInfo.webAbsoluteUrl);
            let list = context.get_web().get_lists().getByTitle('quiz_completion');

            let itemCreateInfo = new SP.ListItemCreationInformation();
            let newItem = list.addItem(itemCreateInfo);

            newItem.set_item('Title', email_id);
            newItem.set_item('isQuizCompleted', isComplete);
            newItem.set_item('quizScore', quizScore);
            newItem.set_item('courseName', course_name);
            newItem.set_item('module', module_name);

            newItem.update();
            context.load(newItem);

            context.executeQueryAsync(
                function () {
                    console.log("Item created successfully. Item ID: " + newItem.get_id());
                },
                function (sender, args) {
                    console.log("Error creating item: " + args.get_message());
                }
            );
        }


        function showTab(tabId, mianId, contenType) {
            console.log(mianId);

            $("#" + tabId).addClass('active').siblings().removeClass('active');
            $("#" + mianId).addClass('active').siblings().removeClass('active');
        }

        function showTab(tabId, mianId, contenType, moudleName, indx) {
            $("#" + tabId).addClass('active').siblings().removeClass('active');
            $("#" + mianId).addClass('active').siblings().removeClass('active');
        }
        // TabIDs 
        async function fetchAndRenderQuizContent(i, moduleName) {
            const items = await fetchDynmQuizzesContent(i, moduleName);
        }
        async function dynTabContents(contents) {
            debugger;
            // console.log('dynamic contents',contents);
            //tabContentsDynamic = "";
            for (let i = 0; i < contents.length; i++) {

                let activeIndex = i == 0 ? " active" : "";

                if (contents[i].content_type == 'Text') {
                    tabContentsDynamic += `
            <div class="tab-content${activeIndex} tabu tabu${i + 1}" id="tab${i + 1}">
                <div class="quize">
                    <div class="quize_qution">
                        ${contents[i].text_content}   
                    </div>
                    <div class="bottom_next">
                        <span class="slide_move">
                            <span style="display:${i == 0 ? "none" : ""}" onclick="prevTab(${i + 1})" id='$prev${i}'> Previous </span>
                            <span style="display:${i== contents.length -1?"none":""}" onclick="nextTab(${i + 1})" id='$next${i}' class="move_next">Next</span>
                            </span>
                    </div>
                </div>
            </div>`;
                } else if (contents[i].content_type == 'Video') {
                    tabContentsDynamic += `
            <div class="tab-content${activeIndex} tabu tabu${i + 1}" id="tab${i + 1}" >
                <video id="video_content${i}" onended="checkVideoCompletion(video, ${contents})" width="640" height="360" style="width: 100%; height: auto;" controls >
                    <source
                        src="${contents[i].video_links}"
                        type="video/mp4">   
                    Your browser does not support the video tag.
                </video>
                <div class="bottom_next">
                    <span class="slide_move">
                        <span style="display:${i == 0 ? "none" : ""}" onclick="prevTab(${i + 1})" id='$prev${i}'> Previous </span>
                        
                        <span style="display:${i == contents.length - 1 ? "none" : ""}" onclick="nextTab(${i + 1})" id='$next${i}' class="move_next">Next</span>
                        </span>
                </div>
            </div>`;
                } else if (contents[i].content_type == 'Quiz') {
                    console.log('content module is', contents[i].module);

                    // Use Promise.all to wait for multiple asynchronous operations
                    // let activeIndex = i == 0 ? " active" : "";
                    tabContentsDynamic += `<div class="tab-content${activeIndex} tabu tabu${i + 1}" id="tab${i + 1}">`;
                    const items = await fetchDynmQuizzesContent(i, contents[i].module);
                    tabContentsDynamic += items;
                    tabContentsDynamic += `</div>`;
                }
            }
            $('#tabContentId').html(tabContentsDynamic);
            courseContents(contents);
        }

        async function courseContents(contents) {
            let dynmContents = "";
            let tabContents = "";
            // dynmContents += `<div class="welcome_course"><h4>${contents[0].module_id}</h4></div>`;
            // let course_name_id = document.getElementById('course_name_id');
            // course_name_id.innerHTML = contents[0].module_id.toUpperCase();

            for (let i = 0; i < contents.length; i++) {

                let iconSVGLocked = `<span><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20" fill="none">
            <g clip-path="url(#clip0_989_1976)">
              <circle cx="10" cy="10" r="10" fill="#1F8354" />
              <path d="M15.8307 5.83398L7.8099 13.8548L4.16406 10.209"
                stroke="white" stroke-width="1.45833" stroke-linecap="round" stroke-linejoin="round" />
            </g>
            <defs>
              <clippath id="clip0_989_1976">
                <rect width="20" height="20" fill="white" />
              </clippath>
            </defs>
          </svg></span>`;
                let activeIndex = i == 0 ? " active" : "";
                console.log('show tab module name', contents[i].module)
                if (contents[i].content_type == 'Text') {
                    dynmContents += `
            <li class="tab${activeIndex}" id="li_tab${i + 1}" onclick="showTab('tab${i + 1}', 'li_tab${i + 1}', '${contents[i].content_type}')">
                <p class="video_desscription_part">
                    <span>
                        ${iconSVGLocked} <span><strong>${contents[i].content_type}: </strong>${decodeURIComponent(contents[i].Title)}</span>
                    </span>
                    <span class="timing">${contents[i].duration} min</span>
                </p>
            </li>`;

                } else if (contents[i].content_type == 'Video') {

                    let currentUserID = _spPageContextInfo.userLoginName;

                    async function fetchDynCourseComplContent() {
                        console.log("called video content currentUserID", currentUserID);
                        try {
                            let apiEndpoint = `https://anheuserbuschinbev.sharepoint.com/sites/Sales-EUR/_api/web/lists/getbytitle('course_completion')/items?$filter=(Title eq '${currentUserID}')`;

                            let response = await fetch(apiEndpoint, {
                                method: "GET",
                                headers: { Accept: "application/json;odata=verbose" },
                            });

                            if (response.ok) {
                                let data = await response.json();
                                // const courseComlData = data.d.results;
                                // lastElapseTime(courseComlData)
                                checkAndUpdateItem(data.d.results);
                                console.log('checkAndUpdateItem course_completion data', data);
                                return data;
                            } else {
                                throw new Error("Error fetching fetchDynmVideoContent data");
                            }
                        } catch (err) {
                            console.error(err);
                        }
                    }


                    let videoMat = contents[i].video_links;
                    let source = document.createElement('source');
                    let video = document.getElementById(`video_content${i}`);

                    source.src = videoMat;
                    source.type = 'video/mp4';

                    if (video) {
                        video.appendChild(source);

                        let startTime = null;
                        let lastUpdateTime = null;
                        let totalElapsedTime = 0;

                        video.addEventListener('play', function () {
                            startTime = new Date();
                            lastUpdateTime = startTime;
                            console.log('Video is playing. Start time:', startTime);
                        });

                        video.addEventListener('timeupdate', function () {
                            if (!video.paused && startTime !== null) {
                                const currentTime = new Date();
                                const elapsedSinceLastUpdate = (currentTime - lastUpdateTime) / 1000;
                                totalElapsedTime += elapsedSinceLastUpdate;
                                lastUpdateTime = currentTime;
                                console.log("Elapsed Time since start: " + totalElapsedTime + " seconds");
                            }
                        });

                        video.addEventListener('pause', function () {
                            // Store the paused time only if the video was already playing
                            if (startTime !== null) {
                                const pausedTime = video.currentTime;
                                console.log('Video is paused. Paused at:', pausedTime);
                                console.log("title is", contents[i].Title)
                                checkAndUpdateItem(totalElapsedTime, 0, contents[i].Title);
                            }
                        });

                        video.addEventListener('ended', function () {
                            // Video playback has ended
                            checkAndUpdateItem(totalElapsedTime, 1, contents[i].Title);
                        });

                        video.addEventListener('play', function () {
                            // If the video was paused before, resume from the paused time
                            if (startTime !== null) {
                                video.currentTime = video.currentTime; // Force update to last played position
                            }
                        });
                    }

                    // Move the checkVideoCompletion function outside the timeupdate event callback
                    var totalDuration = 0;
                    var watchedDuration = 0;
                    function checkVideoCompletion(video, contents) {
                        totalDuration = video.duration;
                        watchedDuration = video.currentTime;

                        if (watchedDuration.toFixed(2) >= totalDuration.toFixed(2)) {
                            console.log("Video completed!");
                            console.log("title is", contents[i].Title)                    // document.getElementsByClassName('tab active').addClass('completed')
                            checkAndUpdateItem(totalDuration, 1, contents[i].Title);
                            // Add your logic here
                        } else {
                            console.log("Video incompleted!");
                            console.log("title is", contents[i].Title)
                            checkAndUpdateItem(watchedDuration, 0, contents[i].Title);
                        }
                    }

                    video.addEventListener('ended', function () {
                        // Handle the end of the video
                        console.log("Video ended!");
                        // Call the checkVideoCompletion function with totalElapsedTime as 0 since the video has ended
                        checkVideoCompletion(video, contents);
                    });

                    async function checkAndUpdateItem(Duration, Completion, course_name) {
                        let urlParams = new URLSearchParams(window.location.search);
                        let param1 = urlParams.get('param1');
                        let email_id = _spPageContextInfo.userLoginName;
                        let apiBaseUrl = _spPageContextInfo.webAbsoluteUrl + `/_api/web/lists/getbytitle('course_completion')/items`;
                        encodedCourseName = encodeURIComponent(course_name);
                        console.log('encode name', encodedCourseName)

                        // Build the filter query to find items associated with the user and matching the title
                        const filterQuery = `$filter=(Title eq '${email_id}' and courseName eq '${encodedCourseName}')`;

                        // Combine the base URL with the filter query
                        const filteredApiUrl = `${apiBaseUrl}?${filterQuery}`;
                        console.log('filteredApiUrl is', filteredApiUrl)

                        try {
                            // Send a GET request to retrieve user-specific items
                            let response = await fetch(filteredApiUrl, {
                                method: 'GET',
                                headers: {
                                    'Accept': 'application/json;odata=verbose',
                                },
                            });

                            if (response.ok) {
                                // Check if an item exists for the user and title
                                let data = await response.json();
                                console.log("video completion data is", data
                                )

                                if (data.d.results.length > 0) {
                                    // Item exists, update it with a PATCH request
                                    const itemId = data.d.results[0].Id;
                                    const updateUrl = `${apiBaseUrl}(${itemId})`;

                                    // Update the existing item
                                    updateListItem(itemId, Duration, Completion);
                                } else {
                                    createListItem(email_id, Duration, encodedCourseName, Completion);
                                }
                            } else {
                                console.error('Error fetching user-specific items');
                            }
                        } catch (error) {
                            console.error(error);
                        }
                    }

                    function updateListItem(itemId, Duration, Completion) {
                        var context = new SP.ClientContext(_spPageContextInfo.webAbsoluteUrl);
                        var list = context.get_web().get_lists().getByTitle('course_completion');
                        var item = list.getItemById(itemId);
                        item.set_item('Time', Duration);
                        item.set_item('isComplete', Completion);
                        item.set_item('isLastWatch', 1);

                        item.update();
                        context.executeQueryAsync(
                            function () {
                                console.log("Item updated successfully.");
                            },
                            function (sender, args) {
                                console.log("Error updating item: " + args.get_message());
                            }
                        );
                    }

                    function createListItem(email_id, Duration, course_name, Completion) {
                        let context = new SP.ClientContext(_spPageContextInfo.webAbsoluteUrl);
                        let list = context.get_web().get_lists().getByTitle('course_completion');

                        let itemCreateInfo = new SP.ListItemCreationInformation();
                        let newItem = list.addItem(itemCreateInfo);

                        newItem.set_item('Title', email_id);
                        newItem.set_item('Time', Duration);
                        newItem.set_item('isComplete', Completion);
                        newItem.set_item('isLastWatch', 1);
                        newItem.set_item('courseName', decodeURIComponent(course_name));

                        newItem.update();
                        context.load(newItem);

                        context.executeQueryAsync(
                            function () {
                                console.log("Item created successfully. Item ID: " + newItem.get_id());
                            },
                            function (sender, args) {
                                console.log("Error creating item: " + args.get_message());
                            }
                        );
                    }

                    dynmContents += `
                        <li class="tab${activeIndex}" id="li_tab${i + 1}" onclick="showTab('tab${i + 1}', 'li_tab${i + 1}', '${contents[i].content_type}')">
                            <p class="video_desscription_part">
                                <span>
                                    ${iconSVGLocked} <span><strong>${contents[i].content_type}: </strong>${decodeURIComponent(contents[i].Title)}
                                </span></span>
                                <span class="timing">${contents[i].duration} min</span>
                            </p>
                        </li>`;
                }
                else if (contents[i].content_type == 'Quiz') {
                    dynmContents += `
                        <li class="tab${activeIndex}" id="li_tab${i + 1}" onclick="showTab('tab${i + 1}', 'li_tab${i + 1}', '${contents[i].content_type}','${contents[i].module}','${i}')">
                            <p class="video_desscription_part">
                                <span>
                                    ${iconSVGLocked} <span><strong>${contents[i].content_type}: </strong>${decodeURIComponent(contents[i].Title)}</span>
                                </span>
                                <span class="timing">${contents[i].duration} min</span>
                            </p>
                        </li>`;
                }
            }
            $('#dynmContents').html(dynmContents);
        }
    </script>
    <!-- Include SweetAlert CDN -->

    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <script>
        var quizScoredCorrect = 0;
        var quizScoredIncorrect = 0;

        function enableSubmitButton(questionIndex, correctAnswer, questL, courseName, module_name, quesID, index) {
            event.preventDefault();
            const checkedOption = $(`input[name="options${quesID}"]:checked`);

            // Check if a radio button is selected
            if (!checkedOption.length) {
                alert("Please select an answer before submitting.");
                return;
            }
            $(`#myForm${quesID} input[type=radio]`).prop('disabled', true);
            selectedOption = checkedOption.val();
            const nextButton = document.getElementById(`next_id${quesID}`);
            let submitButton = $(`#submitButton${quesID}`);
            let label = checkedOption.closest('label');

            if (!selectedOption.length) {
                return;
            }

            const isCorrect = selectedOption.trim() === correctAnswer;

            if (isCorrect) {
                $(`#next_id${questionIndex}`, `#myForm${quesID}`).show();
                submitButton.prop('disabled', false);
                label.addClass('correct');
                label.append('<span class="feedback" style="background: transparent;color: green;">Correct &#10004;</span>');
                nextButton.style.display = 'block';
                document.getElementById(`submitButton${quesID}`).style.display = 'none';
                document.getElementById(`next_id${quesID}`).style.display = 'block';
                quizScoredCorrect++;
                console.log('quizScoredCorrect ', quizScoredCorrect);
            } else {
                $(`input[name="options${quesID}"]`, `#myForm${quesID}`).attr('disabled', false);
                submitButton.prop('disabled', false);
                label.addClass('incorrect');
                label.append('<span class="feedback" style="background: transparent;color: red;">Incorrect &#10008;</span>');
                label.find('input:checked + span.feedback').css('color', 'red');
                console.log($(`input[value="${correctAnswer}"]`));
                const correct = $(`input[value="${correctAnswer}"]`).val();
                console.log(correct);
                $(`input[value="${correctAnswer}"]`, `#myForm${quesID}`).closest('label').append('<span class="feedback" style="background: transparent;color: green;">Correct &#10004;</span>').css('color', 'green');
                nextButton.style.display = 'block';
                document.getElementById(`submitButton${quesID}`).style.display = 'none';
                document.getElementById(`next_id${quesID}`).style.display = 'block';
                quizScoredIncorrect++;
                console.log('quizScoredIncorrect ', quizScoredIncorrect);
            }

            if (questionIndex == questL - 1) {
                console.log('module_name now', module_name);
                console.log('questionIndex ', questL - 1);
                console.log('questionIndex == questL - 1', questionIndex == questL - 1);
                let scroedPercentage = (quizScoredCorrect * 100) / (quizScoredCorrect + quizScoredIncorrect);

                quizCheckAndUpdateItem(1, scroedPercentage.toFixed(2), courseName, module_name);
                fetchDynmQuizzesContent();
                document.getElementById(`next_id${quesID}`).style.display = 'none';
                document.getElementById(`submitButton${quesID}`).style.display = 'none';
                isQuizCompl = true;

                setTimeout(() => {
                    const message = scroedPercentage >= 50
                        ? `You completed the quiz with a score of ${scroedPercentage.toFixed(2)}%.`
                        : 'Unfortunately, you did not clear the quiz.';

                    const icon = scroedPercentage >= 50 ? 'success' : 'error';

                    Swal.fire({
                        title: scroedPercentage >= 50 ? 'Congratulations!' : 'Quiz Not Cleared',
                        text: message,
                        icon: icon,
                        confirmButtonText: 'OK',
                        backdrop: true,
                        backdropOpacity: 0.4
                    }).then((result) => {
                        // Target an existing element outside the popup and add classes
                        const outsideElement = document.querySelector('.your-outside-element-selector'); // Replace with your selector
                        if (outsideElement) {
                            outsideElement.classList.add('banner_top', 'course_over');
                        }

                        if (result.isConfirmed && scroedPercentage >= 50) {
                            console.log('quiz completed', parseInt(index) + 2)
                            let tabElement = document.getElementById(`li_tab${parseInt(index) + 2}`);
                            console.log('tab elmemsts', tabElement)
                            if (tabElement) {
                                tabElement.click();
                            }
                        } else {
                            console.log('quiz completed', parseInt(index))
                            let tabElement = document.getElementById(`li_tab${parseInt(index)}`);
                            console.log('tab elmemsts', tabElement)
                            if (tabElement) {
                                tabElement.click();
                            }
                        }
                    });
                }, 2000);

                quizScoredCorrect = 0;
                quizScoredIncorrect = 0;
            }
        }
    </script>

    <script>
        function nextTab(currentTab) {
            const nextTab = currentTab + 1;
            $("#li_tab"+ nextTab).addClass('active').siblings().removeClass('active')

            // Hide the current tab
            document.querySelector(`.tabu${currentTab}`).classList.remove('active');

            // Show the next tab
            
            const nextTabElement = document.querySelector(`.tabu${nextTab}`);

            if (nextTabElement) {
                nextTabElement.classList.add('active');
            } else {
                // If there is no next tab, you can submit the form or take appropriate action
                alert('Form submitted!');
            }
        }

        function prevTab(currentTab) {
            const prevTab = currentTab - 1;
            $("#li_tab" + prevTab).addClass('active').siblings().removeClass('active')
            // Hide the current tab
            document.querySelector(`.tabu${currentTab}`).classList.remove('active');

            // Show the previous tab
            
            const prevTabElement = document.querySelector(`.tabu${prevTab}`);

            if (prevTabElement) {
                prevTabElement.classList.add('active');
            }
        }
    </script>
</body>

</html>
