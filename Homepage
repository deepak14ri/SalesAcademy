              </div>`;
          }
        }
        $("#dynamicRecommCourseCard").html(user_data_recomm);
        $("#dynamicRecommCourseCard").on("click", ".evenod", function () {
          $(this).toggleClass("icon-color");
        });
        let items = document.querySelectorAll(".carousel .carousel-item");
  
        items.forEach((el) => {
          const minPerSlide = 4;
          let next = el.nextElementSibling;
          for (let i = 1; i < minPerSlide; i++) {
            if (!next) {
              // wrap carousel by using first child
              next = items[0];
            }
            let cloneChild = next.cloneNode(true);
            el.appendChild(cloneChild.children[0]);
            next = next.nextElementSibling;
          }
        });
      }
  
      async function saveToBookmarks(
        ID,
        Title,
        description,
        userId,
        image,
        duration,
        tutor,
        category,
        bookmarks,
        courseId
      ) {
        console.log("Working savetobookmark");
        try {
          // Check if bookmark with the same courseId exists
          let existingBookmark = await getExistingBookmarkByCourseIdAndUserId(
            courseId,
            userId,
            bookmarks
          );
  
          // Check if bookmarks value is 'No' and courseId does not exist
          if (!existingBookmark && bookmarks === "No") {
            await updateItem(
              ID,
              Title,
              description,
              userId,
              image,
              duration,
              tutor,
              category,
              "Yes",
              courseId
            );
            //await createItem(ID, Title, description, userId, image, duration, tutor, category, 'Yes', courseId);
          } else {
            console.log(
              "Bookmark with courseId and userId already exists. Skip creating a new item."
            );
          }
        } catch (err) {
          console.error(err);
        }
      }
  
      async function removeFromBookmarks(ID, Title, category, courseId) {
        var context = new SP.ClientContext(siteUrl);
        var list = context.get_web().get_lists().getByTitle("Video_Overview");
  
        var item = list.getItemById(ID);
  
        // item.deleteObject();
        item.set_item("bookmark_type", "No");
        item.update();
        context.executeQueryAsync(function () {
          console.log("Item updated successfully.");
          fetchDynamicContentRecomm();
        });
      }
  
      async function checkAndUpdate(
        ID,
        Title,
        description,
        userId,
        image,
        duration,
        tutor,
        category,
        bookmarks,
        courseId
      ) {
        try {
          let existingBookmark = await getExistingBookmarkByCourseIdAndUserId(
            courseId,
            userId,
            bookmarks
          );
          if (existingBookmark) {
            updateItem(
              existingBookmark.ID,
              Title,
              description,
              userId,
              image,
              duration,
              tutor,
              category,
              bookmarks,
              courseId
            );
          } else {
            createItem(
              ID,
              Title,
              description,
              userId,
              image,
              duration,
              tutor,
              category,
              bookmarks,
              courseId
            );
          }
        } catch (err) {
          console.error(err);
        }
      }
  
      async function updateItem(
        ID,
        Title,
        description,
        userId,
        image,
        duration,
        tutor,
        category,
        bookmarks,
        courseId
      ) {
        try {
          var listName = "Video_Overview";
          var updateEndpointUrl = `${siteUrl}/_api/web/lists/getbytitle('${listName}')/items(${ID})`;
  
          var context = new SP.ClientContext(_spPageContextInfo.webAbsoluteUrl);
          var list = context.get_web().get_lists().getByTitle(listName);
          var item = list.getItemById(ID);
  
          item.set_item("Title", Title);
          item.set_item("description_content", description);
          item.set_item("user_ids", userId);
          item.set_item("content_img", image);
          item.set_item("duration", duration);
          item.set_item("tutor_name", tutor);
          item.set_item("category_name", category);
          item.set_item("bookmark_type", "Yes");
          item.set_item("course_id", courseId);
  
          item.update();
  
          context.executeQueryAsync(
            function () {
              console.log("Item updated successfully.");
              fetchDynamicContentRecomm();
            },
            function (sender, args) {
              console.log("Error updating item: " + args.get_message());
            }
          );
        } catch (err) {
          console.error(err);
        }
      }
      async function createItem(
        ID,
        Title,
        description,
        userId,
        image,
        duration,
        tutor,
        category,
        bookmarks,
        courseId
      ) {
        try {
          let existingBookmark = await getExistingBookmarkByCourseIdAndUserId(
            courseId,
            userId,
            "Yes"
          );
  
          if (!existingBookmark) {
            var listName = "Video_Overview";
            var context = new SP.ClientContext(siteUrl);
            var list = context.get_web().get_lists().getByTitle(listName);
  
            var itemCreateInfo = new SP.ListItemCreationInformation();
            var newItem = list.addItem(itemCreateInfo);
  
            newItem.set_item("Title", Title);
            newItem.set_item("description_content", description);
            newItem.set_item("user_ids", userId);
            newItem.set_item("content_img", image);
            newItem.set_item("duration", duration);
            newItem.set_item("tutor_name", tutor);
            newItem.set_item("category_name", category);
            newItem.set_item("bookmark_type", "Yes");
            newItem.set_item("course_id", courseId);
  
            newItem.update();
            context.load(newItem);
  
            context.executeQueryAsync(
              function () {
                console.log(
                  "Item created successfully. Item ID: " + newItem.get_id()
                );
  
                fetchDynamicContentRecomm();
              },
              function (sender, args) {
                console.log("Error creating item: " + args.get_message());
              }
            );
          } else {
            console.log(
              "Bookmark with courseId and userId already exists. Skip creating a new item."
            );
          }
        } catch (err) {
          console.error(err);
        }
      }
  
      async function getExistingBookmarkByCourseIdAndUserId(
        courseId,
        userId,
        bookmarks
      ) {
        try {
          debugger;
          let listName = "Video_Overview";
          var endpointUrl = `${siteUrl}/_api/web/lists/getbytitle('${listName}')/items?$filter=course_id eq '${courseId}' and user_ids eq '${userId}' and bookmark_type eq 'Yes'`;
  
          let response = await fetch(endpointUrl, {
            method: "GET",
            headers: { Accept: "application/json;odata=verbose" },
          });
  
          if (response.ok) {
            let data = await response.json();
            if (data.d.results.length > 0) {
              return true;
            } else {
              return false;
            }
          } else {
            throw new Error("Error fetching bookmark data");
          }
        } catch (err) {
          console.error(err);
          return false;
        }
      }
    </script>

    <!-- dynamic session contents -->

    <script>
      // console.log("dynamic script is loaded");
      document.addEventListener("DOMContentLoaded", fetchDynamicContentSession);

      async function fetchDynamicContentSession() {
        try {
          let apiEndpoint =
            "https://anheuserbuschinbev.sharepoint.com/sites/Sales-EUR/_api/web/lists/getbytitle('session_list')/items";
          //?$filter=Category eq 'home_section3'
          let response = await fetch(apiEndpoint, {
            method: "GET",
            headers: { Accept: "application/json;odata=verbose" },
          });

          if (response.ok) {
            let data = await response.json();
            // var items =data.d.results;
            // console.log("data of dynamicCourse ", data);
            renderDynamicContentSession(data.d.results);
          } else {
            throw new Error("Error fetching Dynamic_Contents data");
          }
        } catch (err) {
          console.error(err);
        }
      }

      function renderDynamicContentSession(contents) {
        //console.log("Contents length is:", contents);

        var user_data = "";
        for (let i = 0; i < contents.length; i++) {
          // console.log("contents", contents[i].duration);

          let activeindex = i === 0 ? "active" : "";

          user_data += `<div class="post-slide" ${activeindex}>
            <div class="post-img">
              <img src="${contents[i].image}" alt="">
              <a href="#" class="over-layer"><i class="fa fa-link"></i></a>
              <p> <span>${contents[i].duration} Mins</span> </p>
            </div>
            <div class="post-content">
              <h3 class="post-title">
                <a href="#">${contents[i].Title}</a>
                <p> <span>By ${contents[i].tutor}</span> </p>
              </h3>
              <p class="post-description">
                <a href="#" class="read-more"> <svg xmlns="http://www.w3.org/2000/svg" width="20" height="21" viewBox="0 0 20 21" fill="none">
  <path d="M9.05158 14.0517L13.6753 9.21062L12.7927 8.28125L9.05158 12.1982L7.20684 10.2667L6.32422 11.1962L9.05158 14.0517Z" fill="white" fill-opacity="0.8"/>
  <path fill-rule="evenodd" clip-rule="evenodd" d="M1.875 4.59375H4.375V1.75H5.625V4.59375H14.375V1.75H15.625V4.59375H18.125V18.1562H1.875V4.59375ZM3.125 16.8438V5.90625H16.875V16.8438H3.125Z" fill="white" fill-opacity="0.8"/>
</svg> Add to Calendar</a>
              </p>
            </div>
          </div>`;
        }
        $("#news-slider").html(user_data);
      }
      fetchDynamicContentSession();
    </script>


<!-- top learners -->
<script>
  $(document).ready(function () {
    fetchDynamicContentTopLearners();
  });

  var UserID = _spPageContextInfo.userId;
  var userEmail = _spPageContextInfo.userLoginName;
  // console.log('userEmail _spPageContextInfo', _spPageContextInfo);
  var userPoints, userRank, userTitleName, userDesignation;

  async function fetchDynamicContentTopLearners() {
    try {
      let listName = "course_home";
      let apiEndpoint = `${siteUrl}/_api/web/lists/getbytitle('${listName}')/items`;
      let response = await fetch(apiEndpoint, {
        method: "GET",
        headers: { Accept: "application/json;odata=verbose" },
      });

      if (response.ok) {
        let data = await response.json();
        // console.log("data top learners new", data);
         getTopandBottomprofiles(data.d.results)
      } else {
        throw new Error("Error fetching Dynamic_Contents data");
      }
    } catch (err) {
      console.error(err);
    }
  }

  var currentUserProfile;
  async function getTopandBottomprofiles(data) {
    currentUserProfile = data.filter((item) => item.userId === userEmail);
    const otherUsers = data.filter((item) => item.userId !== userEmail);
    otherUsers.unshift(...currentUserProfile);
    // console.log("otherUsers", otherUsers);
    generateTopSection(currentUserProfile);
    generateBottomSection(otherUsers);
    // console.log('data getTopandBottomprofiles', data);
  }

  async function generateTopSection(data){
    let topsection="";
    data.forEach((item)=>{
      topsection+=`  <li>
        <div class="number">${item.Rank1}<sup>rd</sup></div>
        <div class="user-details">
          <img
            src="https://anheuserbuschinbev.sharepoint.com/sites/Sales-EUR/Assets/images/user-icon.png"
            alt="User"
          />
          <div class="name-designation">
            <span>${item.user_name}</span>
            <p>${item.Designation}</p>
          </div>
        </div>
        <div class="points">${item.Points}</div>
      </li>`
    })
    $("#currentProfile").html(topsection)
  }

  async function generateBottomSection(data){
    let bottomSection="";
    const sortedOtherUsers = data.sort((a, b) => a.Rank1 - b.Rank1);
    // console.log('sortedOtherUsers ranks', sortedOtherUsers);
    // console.log('data ranks', data);
    data.forEach((item)=>{
      if(item.Rank1 !== null) {
        bottomSection+=` <li>
          <div class="number">${item.Rank1}</div>
          <div class="user-details">
            <img src="${item.user_icon}" alt="User">
            <div class="name-designation">
              <span>${item.user_name}</span>
              <p>${item.Designation}</p>
            </div>
          </div>
          <div class="points">${item.Points}</div>
        </li>`
      }
    })
    $("#dynamicTopLearnersBottom").html(bottomSection);
  }

  function getRankSuffix(rank) {
    if (rank >= 11 && rank <= 13) {
      return 'th';
    }
    switch (rank % 10) {
      case 1:
        return 'st';
      case 2:
        return 'nd';
      case 3:
        return 'rd';
      default:
        return 'th';
    }
  }
</script>


  </body>
</html>

