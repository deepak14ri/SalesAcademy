    <script>
        document
            .querySelector("[data-automation-id='contentScrollRegion']")
            .addEventListener("scroll", (event) => {
                if (
                    document.querySelector("[data-automation-id='contentScrollRegion']")
                        .scrollTop > 30
                ) {
                    $("nav").addClass("newclass");
                } else {
                    $("nav").removeClass("newclass");
                }
            });
    </script>
    <script>
        function boldText() {
            var target = document.getElementById("TextArea");
            if (target.style.fontWeight == "bolder") {
                target.style.fontWeight = "normal";
            } else {
                target.style.fontWeight = "bolder";
            }
        }
        function itelicText() {
            var target = document.getElementById("TextArea");
            if (target.style.fontStyle == "italic") {
                target.style.fontStyle = "normal";
            } else {
                target.style.fontStyle = "italic";
            }
        }
    </script>
    <script>
        function addBullet() {
            const bullet = "\u2022";
            const bulletWithSpace = `${bullet} `;
            const enter = 13;

            const handleInput = (event) => {
                const { keyCode, target } = event;
                const { selectionStart, value } = target;

                if (keyCode === enter) {
                    target.value = [...value]
                        .map((c, i) =>
                            i === selectionStart - 1 ? `\n${bulletWithSpace}` : c
                        )
                        .join("");
                    console.log(target.value);

                    target.selectionStart = selectionStart + bulletWithSpace.length;
                    target.selectionEnd = selectionStart + bulletWithSpace.length;
                }

                if (value[0] !== bullet) {
                    target.value = `${bulletWithSpace}${value}`;
                }
            };
        }
    </script>
    <script>
        $(".option_for_user .dot_icon").on("click", function () {
            $(this).next("ul").toggleClass("opened");
        });
    </script>
    <script>

        function generateRandomHexCode() {
            // Generate a random integer between 0 and 16777215 (0xFFFFFF in hexadecimal)
            var randomInt = Math.floor(Math.random() * 16777215);

            // Convert the integer to a hexadecimal string and pad with zeros if needed
            var hexCode = randomInt.toString(16).padStart(6, '0');

            // Prepend a '#' to the hex code
            return hexCode;
        }
        $(document).ready(function () {
            document.getElementById('stopLoading1').addEventListener('click', function (event) {

                // Your additional logic here
            });

            document.getElementById('stopLoading').addEventListener('click', function (event) {
                // Your additional logic here
            });

            // Add event listener for Course Description input
            var courseDescriptionInput = document.getElementById('courseDescription');
            courseDescriptionInput.addEventListener('input', function () {
                var courseDescription = courseDescriptionInput.value;
                if (courseDescription.length > 50) {
                    alert('Course Description should not exceed 50 characters.');
                    // You may also add additional UI feedback or styling here
                }
            });

            // Add event listener for Course Duration input
            var courseDurationInput = document.getElementById('courseDuration');
            courseDurationInput.addEventListener('input', function () {
                var courseDuration = courseDurationInput.value;
                if (!/^\d{1,2}$/.test(courseDuration)) {
                    alert('Course Duration should be a number with a maximum of two digits.');
                    // You may also add additional UI feedback or styling here
                }
            });
            // Disable the button initially
            $('#addContentButton').prop('disabled', true);

            // Function to check if all required fields are filled
            function checkFields() {
                var courseTitle = $('#courseTitle').val();
                var courseDescription = $('#courseDescription').val();
                var courseDuration = $('#courseDuration').val();

                // Check if any checkbox is not checked
                // var anyCheckboxUnchecked = $('.ckkBox').filter(':not(:checked)').length > 0;

                // Enable the button if all fields are filled and all checkboxes are checked
                if (courseTitle && courseDescription && courseDuration) {
                    $('#addContentButton').prop('disabled', false);
                } else {
                    // Otherwise, disable the button
                    $('#addContentButton').prop('disabled', true);
                }
            }

            // Attach the checkFields function to the input and textarea change events
            $('#courseTitle, #courseDescription, #coursetag, #UserCategory, #courseDuration, .ckkBox').on('input change', checkFields);
        });
    </script>
    <script>
        var getFilesofModules = (courseTitle, courseDescription, courseDuration, sessionImage, coursetag, userCategory) => {


            var siteUrl = _spPageContextInfo.webAbsoluteUrl;
            var documents = "";
            var videos = [];
            var images = [];
            var pdfs = [];

            // Document library names
            var parentLibraryName = "coursecontent";
            var subLibraryName = `${courseTitle}`;

            // API endpoint to get files from the nested "Documents" subfolder
            var endpointUrl = siteUrl + `/_api/web/lists/getbytitle('${parentLibraryName}')/items?$expand=File&$filter=FileDirRef eq '/sites/Sales-EUR/${parentLibraryName}/${subLibraryName}'`;

            console.log("endpoint url is ", endpointUrl);

            // Make a GET request to the SharePoint REST API
            $.ajax({
                url: endpointUrl,
                type: "GET",
                headers: {
                    "Accept": "application/json;odata=verbose"
                },
                success: function (data) {
                    // Process the response data (list of files)
                    var files = data.d.results;
                    var all_files = new Array();
                    if (files.length === 0) {
                        console.log('no files')
                        return;
                    }
                    for (let i = 0; i < files.length; i++) {
                        var file = files[i];
                        var fileName = file.File.Name;
                        var fileUrl = "https://anheuserbuschinbev.sharepoint.com" + file.File.ServerRelativeUrl;
                        var fileSizeBytes = file.File.Length;
                        var fileSizeKB = fileSizeBytes / 1024;
                        // Check file extension
                        var fileExtension = fileName.split('.').pop().toLowerCase();
                        if (['mp4', 'avi', 'mkv', 'mov'].includes(fileExtension)) {
                            // Video file
                            videos.push(fileUrl);
                            all_files.push({"fullname": fileName, "name":name, "type":"Video"})
                        } else if (['jpg', 'jpeg', 'png', 'gif'].includes(fileExtension)) {
                            // Image file
                            images.push(fileUrl);
                            all_files.push({ "fullname": fileName, "name": name, "type": "Doc" })
                        } else if (fileExtension === 'pdf') {
                            // PDF file
                            pdfs.push(fileUrl);
                            all_files.push({ "fullname": fileName, "name": name, "type": "pdf" })
                        }
                        // Add other file types if needed
                    }
                    console.log(images)
                    console.log(pdfs)
                    console.log(videos)
                    console.log("all_files", all_files)
                    return ""
                    // Find the array with the highest length
                    var maxArray = Math.max(videos.length, images.length, pdfs.length);
                    console.log('length of max array', maxArray)

                    // Example usage:
                    var randomHexCode = generateRandomHexCode();

                    console.log('rancomecode', randomHexCode)
                    // Iterate through the indexes of the array with the maximum length
                    for (let i = 0; i < maxArray; i++) {
                        // Call createListItem with all file types at the current index
                        createListItem(courseTitle, courseDescription, courseDuration, sessionImage, videos[i] || null, images[i] || null, pdfs[i] || null, i + 1, coursetag, userCategory, randomHexCode);
                    }
                },
                error: function (error) {
                    console.log("Error: " + JSON.stringify(error));
                }
            });
        };

        var save = document.getElementById('saveCourseDetails');

        save.addEventListener('click', function () {


            var courseTitle = document.getElementById('courseTitle').value;
            var courseDescription = document.getElementById('courseDescription').value;
            // var linkToCourse = document.getElementById('linkToCourse').value;
            var courseDuration = document.getElementById('courseDuration').value;
            var courseTagValues = $('.checkboxes#Categories .ckkBox:checked').map(function () {
                return $(this).val();
            }).get().join(',');

            // Retrieve selected User Category checkboxes as a comma-separated string
            var userCategoryValues = $('.checkboxes#Lorems .ckkBox:checked').map(function () {
                return $(this).val();
            }).get().join(',');
            // Get values from the select fields
            // Validate that all required fields are filled
            if (!courseTitle || !courseDescription || !courseDuration || !courseTagValues || !userCategoryValues) {
                alert("Please fill in all required fields.");
                return;
            }

            var element = document.getElementById("course-file");
            var selectedFile = element.files[0];
            if (selectedFile) {
                const fileName1 = selectedFile.name;
                const sessionImage = _spPageContextInfo.webAbsoluteUrl + "/Shared Documents/" + fileName1;
                if (addcontentClick > 0) {
                    getFilesofModules(courseTitle, courseDescription, courseDuration, sessionImage, courseTagValues, userCategoryValues)
                    readsessionFile(courseTitle, sessionImage);
                }
                else {
                    readsessionFile(courseTitle, courseDescription, courseDuration, sessionImage, courseTagValues, userCategoryValues)
                }
            }


        });

        function createListItem(courseTitle, courseDescription, courseDuration, sessionImage, video = null, images = null, pdfs = null, priority, coursetag, userCategory, randomHexCode) {

            let context = new SP.ClientContext(_spPageContextInfo.webAbsoluteUrl);
            let list = context.get_web().get_lists().getByTitle('Video_Overview');

            let itemCreateInfo = new SP.ListItemCreationInformation();
            let newItem = list.addItem(itemCreateInfo);
            newItem.set_item('Title', courseTitle)
            newItem.set_item('category_name', coursetag)
            newItem.set_item('module_id', courseTitle);
            newItem.set_item('description_content', courseDescription);
            newItem.set_item('run_duration', courseDuration);
            newItem.set_item('UserTag', userCategory)
            newItem.set_item('content_priority', parseInt(priority, 10))
            if (video) {
                newItem.set_item('content_type', "Video")
            }
            if (pdfs) {
                newItem.set_item('content_type', "Text")
            }
            if (sessionImage) {
                newItem.set_item("content_img", sessionImage);
            };
            newItem.set_item('pdfLink', pdfs);
            newItem.set_item('video_links', video)
            newItem.set_item('module_id', randomHexCode)
            newItem.set_item('bookmark_type', 'No')


            newItem.update();
            context.load(newItem);

            context.executeQueryAsync(
                function () {
                    console.log("Item created successfully. Item ID: " + newItem.get_id());

                    Swal.fire({
                        title: "updated successfully!",
                        icon: "success"
                    }).then(() => {

                        // Get values from the form
                        var courseTitle = document.getElementById('courseTitle');
                        var courseDescription = document.getElementById('courseDescription');
                        var courseDuration = document.getElementById('courseDuration');


                        var image = document.getElementById("output");

                        // Set all the values to empty strings
                        courseTitle.value = '';
                        courseDescription.value = '';
                        courseDuration.value = '';


                        image.src = '';
                        $('#myModal').modal('hide')
                    });
                },
                function (sender, args) {
                    console.log("Error creating item: " + args.get_message());
                }
            );
        }




    </script>


    <script>

        function readsessionFile(courseTitle, sessionImage) {

            var element = document.getElementById("course-file");
            var file = element.files[0];

            var parts = element.value.split("\\");
            var fileName = parts[parts.length - 1];
            console.log("filename is", fileName);
            //Read File contents using file reader  
            var reader = new FileReader();
            reader.onload = function (e) {
                uploadFile(e.target.result, fileName, courseTitle, sessionImage);
            }
            reader.onerror = function (e) {
                alert(e.target.error);
            }
            reader.readAsArrayBuffer(file);
        }
        function uploadFile(arrayBuffer, fileName, courseTitle, sessionImage) {
            //Get Client Context,Web and List object.  
            var clientContext = new SP.ClientContext();
            var oWeb = clientContext.get_web();
            var oList = oWeb.get_lists().getByTitle('Documents');

            //Convert the file contents into base64 data  
            var bytes = new Uint8Array(arrayBuffer);
            var i, length, out = '';
            for (i = 0, length = bytes.length; i < length; i += 1) {
                out += String.fromCharCode(bytes[i]);
            }
            var base64 = btoa(out);
            //Create FileCreationInformation object using the read file data  
            var createInfo = new SP.FileCreationInformation();
            createInfo.set_content(base64);
            createInfo.set_url(fileName);


            //Add the file to the library  
            var uploadedDocument = oList.get_rootFolder().get_files().add(createInfo)
            // Get the list item associated with the file.
            var listItem = uploadedDocument.get_listItemAllFields();



            //Load client context and execcute the batch  
            clientContext.load(uploadedDocument);
            clientContext.executeQueryAsync(function () {
                console.log('File Uploaded Successfully.');


                // var modal1 = document.getElementById("addexpertModal");
                // modal1.style.display = "none";
            }, QueryFailure);
            function QueryFailure(sender, args) {
                console.log('Request failed with error message - ' + args.get_message() + ' . Stack Trace - ' + args.get_stackTrace());

                // expertImage=_spPageContextInfo.webAbsoluteUrl + "/Shared Documents/default-expert.png"
                // alert("Request failed with error message - " + args.get_message() + " . Stack Trace - " + args.get_stackTrace());
                //createSessionItem()
            }
        }


        function readsessionFile(courseTitle, courseDescription, courseDuration, sessionImage, coursetag, userCategory) {

            var element = document.getElementById("course-file");
            var file = element.files[0];

            var parts = element.value.split("\\");
            var fileName = parts[parts.length - 1];
            console.log("filename is", fileName);
            //Read File contents using file reader  
            var reader = new FileReader();
            reader.onload = function (e) {
                uploadFile(e.target.result, fileName, courseTitle, courseDescription, courseDuration, sessionImage, coursetag, userCategory);
            }
            reader.onerror = function (e) {
                alert(e.target.error);
            }
            reader.readAsArrayBuffer(file);
        }

        function uploadFile(arrayBuffer, fileName, courseTitle, courseDescription, courseDuration, sessionImage, coursetag, userCategory) {
            //Get Client Context,Web and List object.  
            var clientContext = new SP.ClientContext();
            var oWeb = clientContext.get_web();
            var oList = oWeb.get_lists().getByTitle('Documents');

            //Convert the file contents into base64 data  
            var bytes = new Uint8Array(arrayBuffer);
            var i, length, out = '';
            for (i = 0, length = bytes.length; i < length; i += 1) {
                out += String.fromCharCode(bytes[i]);
            }
            var base64 = btoa(out);
            //Create FileCreationInformation object using the read file data  
            var createInfo = new SP.FileCreationInformation();
            createInfo.set_content(base64);
            createInfo.set_url(fileName);


            //Add the file to the library  
            var uploadedDocument = oList.get_rootFolder().get_files().add(createInfo)
            // Get the list item associated with the file.
            var listItem = uploadedDocument.get_listItemAllFields();



            //Load client context and execcute the batch  
            clientContext.load(uploadedDocument);
            clientContext.executeQueryAsync(function () {
                console.log('File Uploaded Successfully.');

                // Example usage:
                var randomHexCode = generateRandomHexCode();

                console.log('rancomecode', randomHexCode)

                createListItem(courseTitle, courseDescription, courseDuration, sessionImage, null, null, null, null, coursetag, userCategory, randomHexCode)

                // var modal1 = document.getElementById("addexpertModal");
                // modal1.style.display = "none";
            }, QueryFailure);
            function QueryFailure(sender, args) {
                console.log('Request failed with error message - ' + args.get_message() + ' . Stack Trace - ' + args.get_stackTrace());

                // Example usage:
                var randomHexCode = generateRandomHexCode();

                console.log('rancomecode', randomHexCode)
                createListItem(courseTitle, courseDescription, courseDuration, sessionImage, null, null, null, null, coursetag, userCategory, randomHexCode)

                // expertImage=_spPageContextInfo.webAbsoluteUrl + "/Shared Documents/default-expert.png"
                // alert("Request failed with error message - " + args.get_message() + " . Stack Trace - " + args.get_stackTrace());
                //createSessionItem()
            }
        }

    </script>
    <script>
