    <script>

        var addcontentClick = 0;
        // Add a click event listener to the "Add Content" button
        document.getElementById('addContentButton').addEventListener('click', function () {
            // Get values from the form
            var courseTitle = document.getElementById('courseTitle').value;
            var courseDescription = document.getElementById('courseDescription').value;
            // var linkToCourse = document.getElementById('linkToCourse').value;
            var courseDuration = document.getElementById('courseDuration').value;

            // Validate that all required fields are filled
            if (!courseTitle || !courseDescription || !courseDuration) {
                alert("Please fill in all required fields.");
                return;
            }
            // Display the values (you can replace this with your desired logic)
            console.log('Course Title: ', courseTitle);
            console.log('Course Description: ', courseDescription);
            // console.log('Link to Course: ', linkToCourse);
            console.log('Course Duration: ', courseDuration);

            // Additional logic to handle file input
            var fileInput = document.getElementById('course-file');
            if (fileInput) {
                if (fileInput.files.length > 0) {
                    var thumbnailFile = fileInput.files[0];
                    console.log('Thumbnail File:', thumbnailFile);
                } else {
                    console.log('No file selected.');
                }
            } else {
                console.error('File input with id "avatar" not found.');
            }
            addcontentClick += 1;
            createDocLib(courseTitle);
        });


        function createDocLib(courseTitle) {
            var clientContext = new SP.ClientContext();
            var parentLibraryTitle = "coursecontent";
            var folderName = courseTitle;
            console.log("foldername is ", folderName)
            var oWebsite = clientContext.get_web();
            var oList = oWebsite.get_lists().getByTitle(parentLibraryTitle);

            var folderCreateInfo = new SP.ListItemCreationInformation();

            folderCreateInfo.set_underlyingObjectType(SP.FileSystemObjectType.folder);

            folderCreateInfo.set_leafName(folderName);

            this.oListItem = oList.addItem(folderCreateInfo);

            this.oListItem.update();

            clientContext.load(oList);

            clientContext.executeQueryAsync(
                Function.createDelegate(this, function () {
                    onDocLibCreationSuccess(folderName);
                }),
                Function.createDelegate(this, this.onDocLibCreationFail)
            );
        }

        function onDocLibCreationSuccess(folderName) {
            console.log(folderName + " Created");
            document.getElementById('uploadContent').style.display = 'block';
            let folderUrl = `https://anheuserbuschinbev.sharepoint.com/sites/Sales-EUR/coursecontent/${folderName}`

            console.log('URL of the created folder:', folderUrl);
            // Add an event listener to the element with id 'targetURL'
            document.getElementById('uploadContent').addEventListener("click", function () {
                // Open the specified URL in a new tab
                debugger;
                window.open(folderUrl, '_blank');

                // Prevent the default behavior of the link (prevents navigation to courseTitle)
                event.preventDefault()
            });
        }

        function onDocLibCreationFail(sender, args) {
            console.log('Failed to Create the Document Library. Error: ' + args.get_message());
            alert("A folder with this name already exists.")
        }


        var getCourseDetails = async () => {

            var requestUri = _spPageContextInfo.webAbsoluteUrl +
                `/_api/web/lists/getByTitle('Video_Overview')/items`;

            try {
                const response = await fetch(requestUri, {
                    method: "GET",
                    headers: {
                        "Accept": "application/json;odata=verbose"
                    }
                });
                if (response.ok) {
                    const data = await response.json();
                    const items = data.d.results;
                    console.log("course items is", items);




                } else {
                    console.error("Request Failed with Status", response.status);
                }
            } catch (error) {
                console.log("Error is", error);
            }
        }

        getCourseDetails();

    </script>
    <script>
        $(document).ready(function () {
            setTimeout(() => {
                $("#news-slider").owlCarousel({
                    items: 3,
                    itemsDesktop: [1199, 3],
                    itemsDesktopSmall: [980, 2],
                    itemsMobile: [600, 1],
                    navigation: true,
                    navigationText: ["", ""],
                    pagination: true,
                    autoPlay: true,
                    dots: true,
                });


                // ------------------- slider first --------------
                let items = document.querySelectorAll(".carousel .carousel-item");

                items.forEach((el) => {
                    const minPerSlide = 4;
                    let next = el.nextElementSibling;
                    for (let i = 1; i < minPerSlide; i++) {
                        if (!next) {
                            // wrap carousel by using first child
                            next = items[0];
                        }
                        let cloneChild = next.cloneNode(true);
                        el.appendChild(cloneChild.children[0]);
                        next = next.nextElementSibling;
                    }
                });
            }, 4000);
        });
    </script>


    <script>
        // console.log("dynamic script is loaded");
        document.addEventListener("DOMContentLoaded", fetchDynamicContentSession);

        async function fetchDynamicContentSession() {
            try {
                let apiEndpoint =
                    "https://anheuserbuschinbev.sharepoint.com/sites/Sales-EUR/_api/web/lists/getbytitle('session_list')/items";
                //?$filter=Category eq 'home_section3'
                let response = await fetch(apiEndpoint, {
                    method: "GET",
                    headers: { Accept: "application/json;odata=verbose" },
                });

                if (response.ok) {
                    let data = await response.json();
                    // var items =data.d.results;
                    // console.log("data of dynamicCourse ", data);
                    renderDynamicContentSession(data.d.results);
                } else {
                    throw new Error("Error fetching Dynamic_Contents data");
                }
            } catch (err) {
                console.error(err);
            }
        }

        function renderDynamicContentSession(contents) {
            //console.log("Contents length is:", contents);

            var user_data = "";
            for (let i = 0; i < contents.length; i++) {
                // console.log("contents", contents[i].duration);

                let activeindex = i === 0 ? "active" : "";

                user_data += `<div class="post-slide" ${activeindex}>
        <div class="post-img">
          <img src="${contents[i].image}" alt="">
          <a href="#" class="over-layer"><i class="fa fa-link"></i></a>
          <p> <span>${contents[i].duration} Mins</span> </p>
        </div>
        <div class="post-content">
          <h3 class="post-title">
            <a href="#">${contents[i].Title}</a>
            <p> <span>By ${contents[i].tutor}</span> </p>
          </h3>
          <p class="post-description">
            <a href="#" class="read-more">Edit/Modify</a>
          </p>
        </div>
      </div>`;
            }
            $("#news-slider").html(user_data);
        }
        fetchDynamicContentSession();
    </script>


    <script>
        $(function () {

            setCheckboxSelectLabels();



            $('.toggle-next').click(function () {
                $(this).next('.checkboxes').slideToggle(400);
            });

            $('.ckkBox').change(function () {
                console.log("called")
                // toggleCheckedAll(this);
                setCheckboxSelectLabels();
                // disableRelatedCheckboxes(this); // Call the new function
            });
            $('.ckkBox').change(function () {
                var currentValue = $(this).val();

                // Handling Advance and Intermediate checkboxes
                if (currentValue === 'Advance' || currentValue === 'Intermediate') {
                    $('.ckkBox[data-disable="' + currentValue + '"]').not(this).prop('disabled', $(this).is(':checked'));
                }

                // Handling On-Trade and Off-Trade checkboxes
                if (currentValue === 'On-Trade') {
                    $('.ckkBox[value="Off-Trade"]').prop('disabled', $(this).is(':checked'));
                } else if (currentValue === 'Off-Trade') {
                    $('.ckkBox[value="On-Trade"]').prop('disabled', $(this).is(':checked'));
                }
            });

        });

        function setCheckboxSelectLabels(elem) {
            var wrappers = $('.wrapper');
            $.each(wrappers, function (key, wrapper) {
                var checkboxes = $(wrapper).find('.ckkBox');
                var label = $(wrapper).find('.checkboxes').attr('id');
                var prevText = '';
                $.each(checkboxes, function (i, checkbox) {
                    var button = $(wrapper).find('button');
                    if ($(checkbox).prop('checked') == true) {
                        var text = $(checkbox).next().html();
                        var btnText = prevText + text;
                        var numberOfChecked = $(wrapper).find('input.val:checkbox:checked').length;
                        if (numberOfChecked >= 4) {
                            btnText = numberOfChecked + ' ' + label + ' selected';
                        }
                        $(button).text(btnText);
                        prevText = btnText + ', ';
                    }
                });
            });
        }

        function toggleCheckedAll(checkbox) {
            var apply = $(checkbox).closest('.wrapper').find('.apply-selection');
            apply.fadeIn('slow');

            var val = $(checkbox).closest('.checkboxes').find('.val');
            var all = $(checkbox).closest('.checkboxes').find('.all');
            var ckkBox = $(checkbox).closest('.checkboxes').find('.ckkBox');

            if (!$(ckkBox).is(':checked')) {
                $(all).prop('checked', true);
                return;
            }

            if ($(checkbox).hasClass('all')) {
                $(val).prop('checked', false);
            } else {
                $(all).prop('checked', false);
            }
        }

        function disableRelatedCheckboxes(checkbox) {
            var disableTarget = $(checkbox).data('disable');
            if (disableTarget) {
                var wrappers = $('.wrapper');
                $.each(wrappers, function (key, wrapper) {
                    if ($(wrapper).find('.checkboxes').attr('id') === disableTarget) {
                        var checkboxesToDisable = $(wrapper).find('.ckkBox').not(checkbox);
                        checkboxesToDisable.prop('disabled', $(checkbox).prop('checked'));
                    }
                });
            }
        }
    </script>

</body>

</html>
